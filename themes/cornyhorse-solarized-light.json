    "editor.foreground": "#657b83",
    "editorLineNumber.foreground": "#93a1a1",
    "editorLineNumber.activeForeground": "#586e75",
    "editorCursor.foreground": "#657b83",
    "editor.selectionBackground": "#eee8d5",
    "editor.selectionHighlightBackground": "#eee8d580",
    "editor.lineHighlightBackground": "#eee8d5",
    "editorWhitespace.foreground": "#eee8d5",
    "editorIndentGuide.background1": "#eee8d5",
    "editorIndentGuide.activeBackground1": "#93a1a1",
    "panelTitle.activeBorder": "#007acc",
    "panelTitle.activeForeground": "#e0e0e0",
    "panelTitle.inactiveForeground": "#cccccc",
    "panel.background": "#1e1e1e",
    "panelTitle.background": "#2d2d30",
    "tab.activeForeground": "#073642",
    "tab.activeBackground": "#fdf6e3",
    "tab.inactiveForeground": "#93a1a1",
    "statusBar.background": "#2d2d30",
    "statusBar.foreground": "#e0e0e0",
    "statusBar.debuggingBackground": "#2d2d30",
    "statusBar.debuggingForeground": "#e0e0e0",
    "statusBar.noFolderBackground": "#2d2d30",
    "statusBar.noFolderForeground": "#e0e0e0",
    "statusBarItem.prominentBackground": "#2d2d30",
    "statusBarItem.errorBackground": "#2d2d30",
    "terminal.background": "#fdf6e3",
    "terminal.foreground": "#657b83",
    "terminalCursor.background": "#fdf6e3",
    "terminalCursor.foreground": "#657b83",
    "terminal.ansiBlack": "#073642",
    "terminal.ansiRed": "#dc322f",
    "terminal.ansiGreen": "#859900",
    "terminal.ansiYellow": "#b58900",
    "terminal.ansiBlue": "#268bd2",
    "terminal.ansiMagenta": "#d33682",
    "terminal.ansiCyan": "#2aa198",
    "terminal.ansiWhite": "#eee8d5",
    "terminal.ansiBrightBlack": "#002b36",
    "terminal.ansiBrightRed": "#cb4b16",
    "terminal.ansiBrightGreen": "#586e75",
    "terminal.ansiBrightYellow": "#657b83",
    "terminal.ansiBrightBlue": "#839496",
    "terminal.ansiBrightMagenta": "#6c71c4",
    "terminal.ansiBrightCyan": "#93a1a1",
    "terminal.ansiBrightWhite": "#fdf6e3",
    "terminal.selectionBackground": "#eee8d580"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#93a1a1",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string.quoted.docstring", "string.quoted.docstring.multi"],
      "settings": {
        "foreground": "#93a1a1",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class"],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "scope": ["support.type"],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "scope": ["support.type.property-name.json", "meta.structure.dictionary.key.json", "string.json support.type.property-name"],
      "settings": {
        "foreground": "#268bd2"
      }
    }
  ]
}
